#!/usr/bin/env bash
# This script displays "Best School" 10 times, and for the 9th iteration, it displays "Hi" on a new line

count=0
while [ $count -lt 10 ]; do
    if [ $count -eq 8 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    count=$((count + 1))
done